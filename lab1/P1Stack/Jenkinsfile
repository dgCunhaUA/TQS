pipeline {
        agent any
        tools {
            jdk 'jdk8'
            maven 'Mvn1'   
        } 
        stages {       
            stage('install') {  
                steps {
                    dir('/lab1/P1Stack'){
                        sh "$PWD"          
                       }
                    }
                steps {               
                    sh 'mvn clean install'                      
                }  
                post {
                    always {
                        junit '**/target/*-reports/TEST-*.xml' 
                    }
                }     
            } 
            
        }
    }